<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <data>

       <!--  Product Synch Local To Server  -->
       <record id="ir_cron_product_updater_local_to_server" model="ir.cron">
            <field name="name">Product Update Local To Server</field>
            <field name="model_id" ref="model_product_template"/>
            <field name="state">code</field>
            <field name="code">model.update_products_local_to_server()</field>
            <field name="interval_number">1</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
       </record>
   </data>
</odoo>
