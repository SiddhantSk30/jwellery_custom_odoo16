<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
        <record id="group_custom_receipt" model="res.groups">
            <field name="name">Receipts</field>
            <field name="category_id" ref="base.module_category_usability"/>
       </record>
        <record id="access_data_comparision" model="ir.model.access">
            <field name="name">Access Receipts</field>
            <field name="model_id" ref="dw_receipts.model_custom_receipt"/>
            <field name="group_id" ref="dw_receipts.group_custom_receipt"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
       </record>
       
       <record id="group_set_to_draft_button" model="res.groups">
            <field name="name">Outward Receipts Set To Draft</field>
            <field name="category_id" ref="base.module_category_usability"/>
       </record>
        <record id="access_set_to_draft" model="ir.model.access">
            <field name="name">Access Outward Receipts Set To Draft</field>
            <field name="model_id" ref="dw_receipts.model_stock_picking"/>
            <field name="group_id" ref="dw_receipts.group_set_to_draft_button"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
       </record>

</odoo>
